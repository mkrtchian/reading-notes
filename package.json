{
  "name": "reading-notes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "npx serve@latest out -s -l 3000",
    "export": "next build --webpack && touch out/.nojekyll",
    "lint": "eslint . --max-warnings=0",
